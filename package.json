{
  "name": "@fnu/poshett-web",
  "version": "0.4.0",
  "description": "poshett-web is the common HTML server and visual interface part of the Poshett project.",
  "keywords": [
    "poshett",
    "music",
    "album",
    "cover"
  ],
  "main": "dist/server/index.js",
  "typings": "dist/server/index.d.ts",
  "scripts": {
    "dev": "run-p dev:**",
    "dev:web": "cross-env NODE_ENV=development webpack --config webpack.config.web.js --mode='development' --watch",
    "dev:server": "cross-env NODE_ENV=development webpack --config webpack.config.server.js --mode='development' --watch",
    "build": "run-s fix-all build:**",
    "build:web": "cross-env NODE_ENV=production webpack --config webpack.config.web.js --mode='production'",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js --mode='production'",
    "start": "node ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "fix-all": "tslint --project tsconfig.web.json --fix src/web/**/*.ts && tslint --project tsconfig.server.json --fix src/server/**/*.ts"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pecamo/Poshett-web.git"
  },
  "dependencies": {
    "@types/express": "4.16.1",
    "@types/node": "10.12.18",
    "@types/ws": "6.0.1",
    "express": "4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "vue": "^2.6.7",
    "vue-router": "^3.0.2",
    "ws": "6.1.3"
  },
  "devDependencies": {
    "@types/vue": "^2.0.0",
    "copyfiles": "2.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.5.4",
    "typescript": "3.2.4",
    "vue-loader": "^15.6.4",
    "vue-template-compiler": "^2.6.7",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1"
  }
}
