{
  "name": "@fnu/poshett-web",
  "version": "0.3.9",
  "description": "poshett-web is the common HTML server and visual interface part of the Poshett project.",
  "keywords": [
    "poshett",
    "music",
    "album",
    "cover"
  ],
  "main": "dist/server/index.js",
  "typings": "dist/server/index.d.ts",
  "scripts": {
    "dev": "npm run watch-ts & npm run watch-sass",
    "build": "npm run build-web & npm run build-server",
    "build-web": "webpack --config webpack.config.web.js",
    "build-server": "webpack --config webpack.config.server.js",
    "dev-web": "webpack --config webpack.config.web.js  --watch",
    "dev-server": "webpack --config webpack.config.server.js  --watch",
    "start": "node ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pecamo/Poshett-web.git"
  },
  "dependencies": {
    "@types/express": "4.16.1",
    "@types/node": "10.12.18",
    "@types/ws": "6.0.1",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "express": "4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "ws": "6.1.3"
  },
  "devDependencies": {
    "copyfiles": "2.1.0",
    "css-loader": "^2.1.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "3.2.4",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  }
}
